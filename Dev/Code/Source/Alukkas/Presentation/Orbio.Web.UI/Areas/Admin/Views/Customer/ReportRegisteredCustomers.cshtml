@model List<Orbio.Web.UI.Areas.Admin.Models.Customers.RegisteredCustomerReportLineModel>
@{
    Layout = null;
}

<div class="main-table table-responsive">
    <h2>REGISTERED CUSTOMERS</h2> <span class="refbutt"></span>

    <table id="data" class="inner-table paginated" cellpadding="0" cellspacing="0" width="100%">
        <thead>
            <tr>
                <th>PERIOD</th>
                <th>COUNT</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var rcr in this.Model)
            {
                <tr>
                    <td>@rcr.Period</td>
                    <td>@rcr.Customers</td>
                </tr>
            }
            @*<tr>
                <td>Total unpaid order (pending payment status)</td>
                <td></td>
            </tr>*@

        </tbody>
    </table>

</div>
