@model List<Orbio.Web.UI.Areas.Admin.Models.Orders.OrderAverageReportLineSummaryModel>
               <div class="chart">
                   <div class="main-table table-responsive">
                       <h2>ORDER TOTALS</h2> <span class="refbutt"></span>

                       <table id="data" class="inner-table paginated" cellpadding="0" cellspacing="0" width="100%">
                           <thead>
                               <tr>
                                   <th>ORDER STATUS</th>
                                   <th>TODAY</th>
                                   <th>THIS WEEK</th>
                                   <th>THIS MONTH</th>
                                   <th>THIS YEAR</th>
                                   <th>ALL TIME</th>
                               </tr>
                           </thead>
                           <tbody>
                               @foreach (var oars in this.Model)
                               {
                                <tr>
                                    <td>@oars.OrderStatus</td>
                                    <td>@oars.CountTodayOrders / @oars.SumTodayOrders</td>
                                    <td>@oars.CountThisWeekOrders / @oars.SumThisWeekOrders</td>
                                    <td>@oars.CountThisMonthOrders / @oars.SumThisMonthOrders</td>
                                    <td>@oars.CountThisYearOrders / @oars.SumThisYearOrders</td>
                                    <td>@oars.CountAllTimeOrders / @oars.SumAllTimeOrders</td>
                                </tr>
                               }
                               @*<tr>
                                   <td></td>
                                   <td></td>
                                   <td>0</td>
                                   <td>0</td>
                                   <td>0</td>
                               </tr>
                               <tr>
                                   <td></td>
                                   <td></td>
                                   <td>0</td>
                                   <td>0</td>
                                   <td>0</td>
                               </tr>
                               <tr>
                                   <td></td>
                                   <td></td>
                                   <td>0</td>
                                   <td>0</td>
                                   <td>0</td>
                               </tr>
                               <tr>
                                   <td></td>
                                   <td></td>
                                   <td>0</td>
                                   <td>0</td>
                                   <td>0</td>
                               </tr>*@

                           </tbody>
                       </table>

                   </div>
               </div>
