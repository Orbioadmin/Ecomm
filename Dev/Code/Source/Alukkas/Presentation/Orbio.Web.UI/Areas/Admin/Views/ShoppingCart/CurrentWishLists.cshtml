
@{
    ViewBag.Title = "Current WishList";
    Layout = "~/Areas/Admin/Views/Shared/_Layout1.cshtml";
}

<div class="container">
    <div class="main_edit_order">
        <span class="edit_order_details"><span class="edit_cart"></span><a href="">Current WishLists</a></span>

    </div>
    <div class="collapsable_table_main">
        @Html.Action("WishlistCustomer")
    </div>
</div>

@section scripts{
    <script type="text/javascript">
        $(document).ready(function () {
            $(".btn_collapse").accordion({
                collapsible: true,
                active: false
            }).on("click", "h3.ui-accordion-header", function (e) {
                $("h3.ui-accordion-header").each(function (i, el) {
                    $(this).find(".panel-icon").text($(el).is(".ui-state-active") ? "-" : "+")
                })
            });

        });


    </script>

