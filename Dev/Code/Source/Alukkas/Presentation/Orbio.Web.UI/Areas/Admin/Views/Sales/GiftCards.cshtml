@{
    Layout = "~/Areas/Admin/Views/Shared/_Layout1.cshtml";
}
@using (Html.BeginForm())
{
    <div class="container">
        <div class="main_edit_order">
            <span class="edit_order_details"><span class="edit_order_icon"></span>Gift Cards</span>
            <span class="back_to_order"><a href="@Url.Action("AddGiftCard","Sales")"><input type="button" value="Add New" class="add_cat_button"></a></span>
        </div>


        <div class="tab-content tab_content_solutions" id="myTabContent">
            <div aria-labelledby="home-tab" id="info" class="tab-pane fade in active  " role="tabpanel">
                @Html.Action("SearchGiftCards", "Sales")

                @Html.Action("ListGiftCards", "Sales")
            </div>
        </div>
    </div>

}
