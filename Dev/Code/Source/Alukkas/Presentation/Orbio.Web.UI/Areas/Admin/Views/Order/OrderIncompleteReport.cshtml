@model List<Orbio.Web.UI.Areas.Admin.Models.Orders.OrderIncompleteReportLineModel>
    @{
        Layout = null;
    }
<div class="main-table table-responsive">
    <h2>INCOMPLETE ORDERS</h2><span class="refbutt"></span>
    <table id="data" class="inner-table paginated" cellpadding="0" cellspacing="0" width="100%">
        <thead>
            <tr>
                <th>ITEM</th>
                <th>COUNT</th>
                <th>TOTAL</th>
            </tr>
        </thead>
        <tbody>
           @foreach (var oir in this.Model)
           {
            <tr>
                <td>@oir.Item</td>
                <td>@oir.Count</td>
                <td>@oir.Total</td>
            </tr>
           }
            @*<tr>
                <td>Total unpaid order (pending payment status)</td>
                <td></td>
                <td>0</td>
            </tr>
            <tr>
                <td>Total not yet shipped orders</td>
                <td></td>
                <td>0</td>
            </tr>
            <tr>
                <td>Total incomplete orders (pending payment status)</td>
                <td></td>
                <td>0</td>
            </tr>*@

        </tbody>
    </table>
</div>