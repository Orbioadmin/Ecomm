@model List<Orbio.Web.UI.Areas.Admin.Models.Customers.CustomerModel>

@{
    Layout = "~/Areas/Admin/Views/Shared/_Layout1.cshtml";
}

<div class="container">
    <div class="main_edit_order">
        <span class="edit_order_details"><span class="shipmen_icon"></span><a href="">Online Customers</a></span>
    </div>
    <table class="order_total_main_table" style="top:7px;">
        <thead>
        <td>Customer Info</td>
        <td>IP Address</td>
        @*<td>Location</td>*@
        <td>Last Activity</td>
        <td>Last Visited Page</td>
        </thead>
        <tbody>
            @if (Model != null)
            {
                if (Model.Count > 0)
                {
                    foreach (var item in Model)
                    {
                        <tr>
                            <td class="align_center">@item.Email</td>
                            <td class="align_center">@item.IPAddress</td>
                            @*<td class="align_center">@item.Location</td>*@
                            <td class="align_center">@item.LastActivity</td>
                            <td class="align_center">@item.LastVisitedPage</td>
                        </tr>
                    }
                }
                else
                {
                    <tr style="display: table-row;">

                        <td colspan="6">No records to display</td>
                    </tr>
                }
            }
            else
            {
                <tr style="display: table-row;">

                    <td colspan="6">No records to display</td>
                </tr>
            }
        </tbody>
    </table>
</div>