            $('.scroll_top').click(function (event) {
                event.preventDefault();

                $('html, body').animate({ scrollTop: 0 }, 300);
            })
        });

        // Enable the Tabs

        $('#myTabs a').click(function (e) {
            e.preventDefault()
            $(this).tab('show')
